{
  "name": "vue-3-ts",
  "author": "Timofei Ustyantsev",
  "private": true,
  "scripts": {
    "setup": "git config blame.ignoreRevsFile .git-blame-ignore-revs",
    "dev": "vite --host",
    "dev:mock": "VITE_MOCK_API=1 vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build:mock": "VITE_MOCK_API=1 yarn build",
    "build:v": "ROLLUP_BUNDLE_VISUALIZE=1 yarn build",
    "start": "yarn && yarn build && yarn serve",
    "start:mock": "yarn && yarn build:mock && yarn serve",
    "start:prod": "yarn && yarn build && yarn serve:prod",
    "serve": "vite preview --port 8080 --host",
    "serve:prod": "http-server dist -P http://localhost:8080?",
    "stage": "yarn && yarn build",
    "prod": "yarn && vue-tsc --noEmit && vite build && yarn start:prod",
    "lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:fix": "yarn lint -- --fix",
    "hooks:install": "husky install",
    "hooks:pre-commit": "lint-staged",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "ci:test": "WAIT_ON_TIMEOUT=600000 start-server-and-test start:mock http-get://localhost:8080 cy:run"
  },
