{
  "name": "nuxt-vue-2-ts",
  "author": "Timofei Ustyantsev",
  "private": true,
  "scripts": {
    "setup": "git config blame.ignoreRevsFile .git-blame-ignore-revs",
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "nuxt build",
    "build:v": "WEBPACK_BUNDLE_ANALYZE=1 nuxt build",
    "serve": "yarn build --devtools && yarn start",
